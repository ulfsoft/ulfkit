{
    "name": "@ulfsoft/ulfkit",
    "version": "0.0.1",
    "description": "A toolkit for building resilient and scalable applications.",
    "private": false,
    "type": "module",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "import": "./dist/index.mjs",
            "require": "./dist/index.cjs",
            "types": "./dist/index.d.ts"
        }
    },
    "files": [
        "dist"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "build:watch": "tsc -w -p tsconfig.json",
        "typecheck": "tsc --noEmit -p tsconfig.json",
        "clean": "rimraf dist",
        "lint": "biome check .",
        "prepare": "pnpm build",
        "prepublishOnly": "pnpm run build && pnpm run typecheck"
    },
    "publishConfig": {
        "access": "public"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/ulfsoft/ulfkit.git"
    },
    "bugs": {
        "url": "https://github.com/ulfsoft/ulfkit/issues"
    },
    "homepage": "https://github.com/ulfsoft/ulfkit#readme",
    "keywords": [],
    "author": "Ulfsoft Krzysztof Begiedza",
    "license": "Apache-2.0",
    "packageManager": "pnpm@10.13.1",
    "devDependencies": {
        "@biomejs/biome": "2.2.4",
        "rimraf": "^6.0.1",
        "typescript": "^5.6.3"
    }
}
